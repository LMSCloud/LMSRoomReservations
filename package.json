{
  "name": "koha-plugin-lms-room-reservations",
  "version": "4.0.0",
  "previous_version": "3.4.20",
  "minimum_koha_version": "21.05.14.000",
  "description": "Koha Room Reservations Plugin",
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "eslint": "^8.20.0",
    "eslint-plugin-lit": "^1.8.2",
    "eslint-plugin-lit-a11y": "^2.3.0",
    "eslint-plugin-wc": "^1.4.0",
    "gulp": "^4.0.2",
    "gulp-cli": "^1.2.2",
    "gulp-github-release": "^1.1.5",
    "gulp-run": "^1.7.1",
    "postcss": "^8.4.21",
    "postcss-lit": "^1.0.1",
    "rollup": "^2.77.2",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-html-literals": "^1.1.6",
    "rollup-plugin-minify-html-literals": "^1.2.6",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "stylelint": "^14.16.1",
    "stylelint-config-standard": "^29.0.0"
  },
  "scripts": {
    "clean": "rm *.kpz",
    "build": "./node_modules/rollup/dist/bin/rollup -c && gulp build",
    "build:next": "./node_modules/rollup/dist/bin/rollup -c && node increment_version.js && gulp build",
    "docker:dev": "npm run clean && npm run build && npm run docker:move && npm run docker:unzip && npm run docker:install",
    "docker:clean": "docker exec -it koha-koha-1 rm /var/lib/koha/kohadev/plugins/*.kpz",
    "docker:move": "docker cp *.kpz koha-koha-1:/var/lib/koha/kohadev/plugins/",
    "docker:unzip": "docker exec -it koha-koha-1 unzip -o /var/lib/koha/kohadev/plugins/*.kpz",
    "docker:install": "docker exec -it koha-koha-1 /kohadevbox/koha/misc/devel/install_plugins.pl",
    "lint:css": "stylelint './src/components/*.js'",
    "LMSCalendar:init": "git submodule update --init --recursive",
    "LMSCalendar:pull": "git submodule update --recursive --remote"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LMSCloud/LMSRoomReservations.git"
  },
  "author": "Paul Derscheid <paul.derscheid@lmscloud.de>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/LMSCloud/LMSRoomReservations/issues"
  },
  "homepage": "https://github.com/LMSCloud/LMSRoomReservations#readme",
  "dependencies": {
    "@granite-elements/granite-lit-bootstrap": "^4.6.0-0",
    "@granite-elements/granite-lit-spectre": "^0.5.9-2",
    "datetime": "0.0.3",
    "dayjs": "^1.11.7",
    "gulp-replace": "^0.6.1",
    "jsonfile": "^4.0.0",
    "lit": "^2.4.1",
    "node-datetime": "^2.0.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0"
  }
}
